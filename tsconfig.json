{
  "compilerOptions": {
    "target": "ES2020",                
    "module": "ESNext",                
    "lib": ["esnext"],
    "outDir": "dist",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",        
    "resolveJsonModule": true,        
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
